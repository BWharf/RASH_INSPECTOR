   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "for file in glob.glob('/Users/waranthornchansawang/Desktop/new_data/image/*.png'):\n",
    "    im = Image.open(file)\n",
    "    rgb_im = im.convert('RGB')\n",
    "    rgb_im.save(file.replace(\"png\", \"jpg\"), quality=100)"
